	.data
	.text
	.global main
main:
	addi sp, sp, -48
main_entry:
#   @a = alloc i32
	addi t0, sp, 0
	sw t0, 36(sp)
#   store 1, @a
	lw t0, 36(sp)
	li t1, 1
	sw t1, 0(t0)
#   @b = alloc i32
	addi t0, sp, 4
	sw t0, 32(sp)
#   store 2, @b
	lw t0, 32(sp)
	li t1, 2
	sw t1, 0(t0)
#   jump %entry_0
	jal x0, main_entry_0
main_entry_0:
#   @a_0 = alloc i32
	addi t0, sp, 8
	sw t0, 28(sp)
#   store 2, @a_0
	lw t0, 28(sp)
	li t1, 2
	sw t1, 0(t0)
#   %0 = load @b
	lw t0, 32(sp)
	lw t0, 0(t0)
	sw t0, 24(sp)
#   %1 = load @a_0
	lw t0, 28(sp)
	lw t0, 0(t0)
	sw t0, 20(sp)
#   %2 = add %0, %1
	lw t1, 24(sp)
	lw t2, 20(sp)
	add t0, t1, t2
	sw t0, 16(sp)
#   store %2, @b
	lw t0, 32(sp)
	lw t1, 16(sp)
	sw t1, 0(t0)
#   jump %entry_1
	jal x0, main_entry_1
main_entry_1:
#   %3 = load @b
	lw t0, 32(sp)
	lw t0, 0(t0)
	sw t0, 12(sp)
#   ret %3
	lw t0, 12(sp)
	addi a0, t0, 0
	addi sp, sp, 48
	ret

