														MulExp : UnaryExp 
															UnaryExp : PrimaryExp 
																PrimaryExp : Number 
																	Number : Integer 
																		Integer : DEC_INTEGER 
																			DEC_INTEGER = "2"
														"*" = "*"
														UnaryExp : PrimaryExp 
															PrimaryExp : Number 
																Number : Integer 
